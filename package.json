{
  "name": "rouxl-roles",
  "version": "0.0.1",
  "description": "A automatic role assigning bot to assign roles through interactions.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run lint && npm run prettier && npm run compile",
    "test": "eslint src && prettier --check src/**/*.ts && tsc --noEmit",
    "lint": "eslint src/**/*.ts --fix",
    "prettier": "prettier --write src/**/*.ts",
    "compile": "tsc",
    "start": "npm ci && npm run build && node ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NotReallyEight/rouxl-roles.git"
  },
  "author": "NotReallyEight",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/NotReallyEight/rouxl-roles/issues"
  },
  "homepage": "https://github.com/NotReallyEight/rouxl-roles#readme",
  "dependencies": {
    "@types/node": "^16.11.18",
    "chalk": "^5.0.0",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.5.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.6.0",
    "prettier": "^2.5.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.8.1"
  }
}
